generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
  username String
}
